{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ali60351/material-rte.git"
  },
  "homepage": "https://github.com/Ali60351/material-rte#readme",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "deploy": "gh-pages -d example/dist"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxSingleQuote": false,
    "semi": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "tabWidth": 4,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "name": "material-rte",
  "author": "Ali Nauman",
  "module": "dist/material-rte.esm.js",
  "size-limit": [
    {
      "path": "dist/material-rte.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/material-rte.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "classnames": "^2.3.1",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "formik": "^2.2.9"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/transformer-raw": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/draft-js": "^0.11.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
